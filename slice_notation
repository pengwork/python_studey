>>> arr = [1, 2, 3, 4, 5]    
>>> arr[:] 
[1, 2, 3, 4, 5]
>>> arr[0:] 
[1, 2, 3, 4, 5]
>>> arr[3:] 
[4, 5]
>>> arr[:5] 
[1, 2, 3, 4, 5]
>>> arr[:3] 
[1, 2, 3]
>>> arr[:10] 
[1, 2, 3, 4, 5]
>>> arr[1:4]
[2, 3, 4]
>>> arr[1:4:2]
[2, 4]
>>> arr[1::2]
[2, 4]
>>> arr[:4:2]
[1, 3]
>>> arr[::2]
[1, 3, 5]
>>> arr[::-2]
[5, 3, 1]
>>> arr[-3:-5:-2]
[3]
>>> arr[-3::-2]
[3, 1]
>>> arr[-3:-6:-2]
[3, 1]
